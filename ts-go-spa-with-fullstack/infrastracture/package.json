{
  "name": "infrastracture",
  "version": "0.1.0",
  "bin": {
    "infrastracture": "bin/infrastracture.js"
  },
  "scripts": {
    "build:frontend:dev": "cd ../web && npm install && npm run build",
    "build:frontend:stg": "cd ../web && npm install && npm run build:stg",
    "build:frontend:prod": "cd ../web && npm install && npm run build:prod",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "deploy:dev": "NODE_ENV=development STAGE=dev DOTENV_PATH=../.env.dev cdk deploy --all",
    "synth:dev": "NODE_ENV=development STAGE=dev DOTENV_PATH=../.env.dev cdk synth --all",
    "destroy:dev": "NODE_ENV=development STAGE=dev DOTENV_PATH=../.env.dev cdk destroy --all --force"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.173.2",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.726.1",
    "aws-cdk-lib": "2.173.2",
    "constructs": "^10.0.0",
    "zod": "^3.24.1"
  }
}
